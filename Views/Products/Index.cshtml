@model IEnumerable<CuttingEdge.Models.Product>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Cutting Edge Marketplace</h2>

<p>
    @Html.ActionLink("Add Video", "New")
</p>

<ul>
